{
  "name": "CryptoResume",
  "description": "Small extension for save your cryptos and see his values",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage"],
  "host_permissions": ["https://api.coingecko.com/api/*"],
  "icons": {
    "128": "/images/coingLogo.png"
  },
  "options_page": "options.html",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "/images/coingLogo.png"
    }
  }
}
