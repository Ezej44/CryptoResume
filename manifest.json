{
  "name": "Crypto resume",
  "description": "Small extension for saving your favorite cryptos and see their current price",
  "version": "1.0.4",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage"],
  "host_permissions": ["https://api.coingecko.com/api/*"],
  "icons": {
    "128": "/images/logo.png"
  },
  "options_page": "options.html",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "/images/logo.png"
    }
  }
}
